#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/HDD/Web Dev Projects/GITHUB FIRMOWY/Krapi/api-server/node_modules/prebuild-install/node_modules:/mnt/e/HDD/Web Dev Projects/GITHUB FIRMOWY/Krapi/api-server/node_modules:/mnt/e/HDD/Web Dev Projects/GITHUB FIRMOWY/Krapi/node_modules:/mnt/e/HDD/Web Dev Projects/GITHUB FIRMOWY/node_modules:/mnt/e/HDD/Web Dev Projects/node_modules:/mnt/e/HDD/node_modules:/mnt/e/node_modules:/mnt/e/HDD/Web Dev Projects/GITHUB FIRMOWY/Krapi/api-server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/e/HDD/Web Dev Projects/GITHUB FIRMOWY/Krapi/api-server/node_modules/prebuild-install/node_modules:/mnt/e/HDD/Web Dev Projects/GITHUB FIRMOWY/Krapi/api-server/node_modules:/mnt/e/HDD/Web Dev Projects/GITHUB FIRMOWY/Krapi/node_modules:/mnt/e/HDD/Web Dev Projects/GITHUB FIRMOWY/node_modules:/mnt/e/HDD/Web Dev Projects/node_modules:/mnt/e/HDD/node_modules:/mnt/e/node_modules:/mnt/e/HDD/Web Dev Projects/GITHUB FIRMOWY/Krapi/api-server/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../prebuild-install/bin.js" "$@"
else
  exec node  "$basedir/../prebuild-install/bin.js" "$@"
fi
