#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/HDD/Web Dev Projects/GITHUB FIRMOWY/Krapi/api-server/node_modules/.pnpm/jest@30.0.4_@types+node@22.15.31_ts-node@10.9.2_@types+node@22.15.31_typescript@5.8.3_/node_modules/jest/bin/node_modules:/mnt/e/HDD/Web Dev Projects/GITHUB FIRMOWY/Krapi/api-server/node_modules/.pnpm/jest@30.0.4_@types+node@22.15.31_ts-node@10.9.2_@types+node@22.15.31_typescript@5.8.3_/node_modules/jest/node_modules:/mnt/e/HDD/Web Dev Projects/GITHUB FIRMOWY/Krapi/api-server/node_modules/.pnpm/jest@30.0.4_@types+node@22.15.31_ts-node@10.9.2_@types+node@22.15.31_typescript@5.8.3_/node_modules:/mnt/e/HDD/Web Dev Projects/GITHUB FIRMOWY/Krapi/api-server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/e/HDD/Web Dev Projects/GITHUB FIRMOWY/Krapi/api-server/node_modules/.pnpm/jest@30.0.4_@types+node@22.15.31_ts-node@10.9.2_@types+node@22.15.31_typescript@5.8.3_/node_modules/jest/bin/node_modules:/mnt/e/HDD/Web Dev Projects/GITHUB FIRMOWY/Krapi/api-server/node_modules/.pnpm/jest@30.0.4_@types+node@22.15.31_ts-node@10.9.2_@types+node@22.15.31_typescript@5.8.3_/node_modules/jest/node_modules:/mnt/e/HDD/Web Dev Projects/GITHUB FIRMOWY/Krapi/api-server/node_modules/.pnpm/jest@30.0.4_@types+node@22.15.31_ts-node@10.9.2_@types+node@22.15.31_typescript@5.8.3_/node_modules:/mnt/e/HDD/Web Dev Projects/GITHUB FIRMOWY/Krapi/api-server/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../jest/bin/jest.js" "$@"
else
  exec node  "$basedir/../jest/bin/jest.js" "$@"
fi
